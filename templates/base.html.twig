<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Squelette d'application symfony 5.4 + react.js!{% endblock %}</title>
        <script src="accessconfig/js/accessconfig.min.js" type="text/javascript"></script>
        <link rel="stylesheet" href="accessconfig/css/accessconfig.min.css" type="text/css" media="screen" />
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}

        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>

    <header>
         <div id="accessconfig" data-accessconfig-buttonname="Paramètres d’accessibilité" data-accessconfig-params='{ "Prefix" : "a42-ac", "ContainerClass" : "","ModalCloseButton" : "","ModalTitle" : "","FormFieldset" : "","FormFieldsetLegend" : "","FormRadio" : ""}' >
             <button title="Parametre d'accessibilité" class="bg-transparent border-0 "
                     data-accessconfig-button="true">
                 <i aria-label="Accessibilité" class="bi bi-universal-access-circle a11y-icon"></i>
             </button>
         </div>
        <nav><a href="/">HOME</a> </nav>
    </header>
    <main>
        {% block main %}{% endblock %}
    </main>
    </body>
</html>
